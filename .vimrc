filetyp on
filetype plugin on
filetyp indent on
syntax on

set hlsearch
set incsearch
set ignorecase
set smartcase

set expandtab
set tabstop=3
set shiftwidth=3
set softtabstop=3
set smartindent
set autoindent

set shortmess=atI

set guioptions=a
set scrolloff=10

set history=1000

"Make file/command completion complete like the shell"
set wildmenu

let g:lisp_rainbow=1
let g:paredit_shortmaps=1
